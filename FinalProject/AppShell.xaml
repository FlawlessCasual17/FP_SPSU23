<?xml version="1.0" encoding="UTF-8" ?>

<!--  ReSharper disable once Xaml.RedundantPropertyTypeQualifier  -->
<Shell
    x:Class="FinalProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:finalProject="using:FinalProject"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate finalProject:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Sign Up"
        ContentTemplate="{DataTemplate finalProject:SignUp}"
        Route="SignUp" />

    <ShellContent
        Title="Log In"
        ContentTemplate="{DataTemplate finalProject:Login}"
        Route="LogIn" />

    <ShellContent
        Title="ManageMember"
        ContentTemplate="{DataTemplate finalProject:ManageMember}"
        Route="ManageMember" />

</Shell>
